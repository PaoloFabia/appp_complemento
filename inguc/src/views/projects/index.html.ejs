<div class="cabeza-contenedor">
  <div id="titulo-contenedor">
    <h1>PROYECTOS CAi</h1>
  </div>
  <div id="informacion-extra">
    <% if (!projectsList.length) { %>
      <p>No hay inventario</p>
      
    <% } else { %>
      
  </div>



</div>


<div class="cuerpo-contenedor">




  <% projectsList.forEach((project) => { %>

    <div class="contenedor-item">

      <div class="divnombre">
        <h3>
          <%= project.name %>
        </h3>
      </div>

      <div class="item-imagenes" id="imagen-objeto">
        <a href="<%= profilePath(project) %>">
          <img src="<%=project.image%>">
        </a>

      </div>

      <div class="piso">
        <a href="<%= profilePath(project) %>">
          <img src="https://yplqsmdlg-images-1.s3.amazonaws.com/moreinfo.png">
        </a>


      </div>

    </div>
    <% }) %>

</div>

<% } %>
